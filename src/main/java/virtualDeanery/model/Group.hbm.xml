<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="virtualDeanery.model">
    <class name="Group" table="GROUPS">
        <id name="group_id" type="java.lang.String" access="field">
            <column name="GROUP_ID" />
            <generator class="assigned" />
        </id>
        <property name="acronym_subject" type="java.lang.String">
            <column name="ACRONYM_SUBJECT" />
        </property>
        <property name="time" type="java.lang.String">
            <column name="TIME" />
        </property>
        <property name="niu_instructor" type="int">
            <column name="NIU_INSTRUCTOR" />
        </property>
    </class>
</hibernate-mapping>
